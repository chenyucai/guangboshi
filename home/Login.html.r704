<script>
	$(document).ready(function() {

		})
		//	if($.cookie("userId") != null) {
		////		alert("登陆成功");
		//		location.href = "index.html"
		//	}
	$("#submit").click(function() {
		if ($.trim($("#phone").val()).length === 0) {
			alert('请输入手机号');
			return ;
		}
		if ($.trim($("#PassWord").val()).length === 0) {
			alert('请输入密码');
			return ;
		}
		$.get(constants.baseUrl + "/AppLogin/Login?Mobile=" + $("#phone").val() + "&Password=" + $("#PassWord").val(), function(data, status) 
			//			$.cookie(data.ProviderKey,JSON.stringify(data.operatorModel),{Path:constants.baseUrl+"/"});
			if(data.Status == 1) {
				location.href = "index.html";
				
				$.cookie('userId', data.UserId, {
					expires: 7,
					path: '/'
				});
				$.cookie('CityId', "", {
					expires: 7
				});
			}
			else if(data.Status != 1) {
				alert(data.Message);
			}
		})
	})

	$("#eye").click(function() {
		if($("#PassWord").val() != "") {
			if($("#PassWordSee").css("display") == "none") {
				$("#PassWordSee").val($("#PassWord").val());
				$("#PassWordSee").css("display", "block");
				$("#PassWord").css("display", "none");
				$("#eyeChange").attr("src", "../img/1/icon_display@3x.png")
			} else {
				$("#PassWord").css("display", "block");
				$("#PassWordSee").css("display", "none");
				$("#eyeChange").attr("src", "../img/1/icon_Not show@3x.png")
			}
		}

	})

	var vCode = getQueryString("code");
	if(vCode != "" && vCode != null) {
		//		alert(vCode);
		debugger;
		localStorage.setItem("code", vCode);
	}

	function getQueryString(name) {
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
		var r = window.location.search.substr(1).match(reg);
		if(r != null) return unescape(r[2]);
		return null;
	}

	function yz(v) {
		//var a = /^((/ / ( / /d{3}//))|(//d{3}//-))?13//d{9}|15[89]//d{8}$/ ;
		var a = /^1\d{10}$/;
		if(v.length != 11 || !v.match(a)) {
			alert("请输入正确的手机号码");
		} else {;
		}
	}

//	$(".activityBlock").live("click", function() {
//		location.href = "signin.html"
//	})
</script>
